  # Docker Compose .env file (NumerusX/.env)

   # Backend Configuration
   SOLANA_RPC_URL=https://api.devnet.solana.com
   SOLANA_PRIVATE_KEY_BS58=YOUR_DUMMY_PRIVATE_KEY_DO_NOT_USE_FOR_REAL_TRANSACTIONS
   GOOGLE_API_KEY=DUMMY_GOOGLE_API_KEY
   JUPITER_PRO_API_KEY=DUMMY_JUPITER_PRO_KEY
   API_PORT=8000
   GEMINI_MODEL_NAME=gemini-1.5-flash-latest
   GEMINI_API_TIMEOUT_SECONDS=60
   GEMINI_MAX_TOKENS_INPUT=4096
   GEMINI_MAX_TOKENS_OUTPUT=2048
   DEBUG_PROMPTS=False
   GEMINI_INPUT_COST_PER_MILLION_TOKENS=0.35
   GEMINI_OUTPUT_COST_PER_MILLION_TOKENS=1.05


   # Frontend Configuration (these will be available to the frontend container if needed at runtime)
   # Vite primarily uses its own .env file during its build process (numerusx-ui/.env)
   VITE_APP_AUTH0_DOMAIN=dummy-placeholder.auth0.com
   VITE_APP_AUTH0_CLIENT_ID=DUMMY_AUTH0_CLIENT_ID
   VITE_APP_AUTH0_AUDIENCE=DUMMY_API_AUDIENCE
   VITE_APP_SOCKET_URL=http://localhost:8000
   VITE_APP_BACKEND_URL=http://localhost:8000