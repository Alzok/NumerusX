# NumerusX Backend Configuration (.env)
# =====================================
# Ce fichier contient UNIQUEMENT les variables backend
# Variables frontend dans numerusx-ui/.env

# APIs Externes (OBLIGATOIRE - remplacer par vos clés)
# ===================================================
GOOGLE_API_KEY=DUMMY_GOOGLE_API_KEY
JUPITER_API_KEY=DUMMY_JUPITER_API_KEY
JUPITER_PRO_API_KEY=DUMMY_JUPITER_PRO_KEY
DEXSCREENER_API_KEY=DUMMY_DEXSCREENER_KEY
SOLANA_PRIVATE_KEY_BS58=YOUR_DUMMY_PRIVATE_KEY_DO_NOT_USE_FOR_REAL_TRANSACTIONS
SOLANA_RPC_URL=https://api.devnet.solana.com

# Sécurité (OBLIGATOIRE - générer vos propres clés)
# =================================================
JWT_SECRET_KEY=change-this-secret-key-in-production
MASTER_ENCRYPTION_KEY=change-this-32-char-encryption-key-fernet

# Auth0 Backend (OBLIGATOIRE - configurer votre Auth0)
# ===================================================
AUTH_PROVIDER_JWKS_URI=https://your-domain.auth0.com/.well-known/jwks.json
AUTH_PROVIDER_ISSUER=https://your-domain.auth0.com/
AUTH_PROVIDER_AUDIENCE=your-api-identifier
AUTH_PROVIDER_ALGORITHMS=RS256

# Configuration Application
# ========================
APP_NAME=NumerusX
DEBUG=False
DEV_MODE=False
API_HOST=0.0.0.0
API_PORT=8000
CORS_ALLOWED_ORIGINS=*

# Base de Données
# ==============
DATABASE_URL=sqlite:///data/numerusx.db
DB_PATH=data/numerusx.db

# Redis (Cache et Rate Limiting)
# ==============================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_URL=redis://localhost:6379/0

# Google Gemini AI
# ===============
GEMINI_MODEL_NAME=gemini-2.5-flash-preview-05-20
GEMINI_API_TIMEOUT_SECONDS=60
GEMINI_MAX_TOKENS_INPUT=4096
GEMINI_MAX_TOKENS_OUTPUT=2048
DEBUG_PROMPTS=False
GEMINI_INPUT_COST_PER_MILLION_TOKENS=0.35
GEMINI_OUTPUT_COST_PER_MILLION_TOKENS=1.05

# Jupiter Trading API
# ==================
JUPITER_LITE_API_HOSTNAME=https://lite-api.jup.ag
JUPITER_PRO_API_HOSTNAME=https://api.jup.ag
JUPITER_DEFAULT_SLIPPAGE_BPS=50
JUPITER_DYNAMIC_COMPUTE_UNIT_LIMIT=True
JUPITER_PRIORITY_FEE_LEVEL=Default
JUPITER_WRAP_AND_UNWRAP_SOL=True
JUPITER_ONLY_DIRECT_ROUTES=False
JUPITER_MAX_RETRIES=3

# DexScreener API
# ==============
DEXSCREENER_API_URL=https://api.dexscreener.com/latest/dex

# Trading Parameters
# =================
BASE_ASSET=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
SLIPPAGE_BPS=50
MIN_LIQUIDITY_USD=10000
MAX_OPEN_POSITIONS=5
MAX_ORDER_SIZE_USD=1000.0
TRADE_CONFIDENCE_THRESHOLD=0.65
TRADING_UPDATE_INTERVAL_SECONDS=60
INITIAL_PORTFOLIO_BALANCE_USD=1000.0
MIN_ORDER_VALUE_USD=10.0

# Logs
# ====
LOG_DIR=logs
LOG_FILE_NAME=numerusx.log
LOG_LEVEL=INFO
LOG_MAX_MSG_LENGTH=4000

# UI Updates
# ==========
UI_UPDATE_INTERVAL_SECONDS=2
RUN_PERIODIC_SOCKET_UPDATES=False

# Security Checker
# ===============
SECURITY_MAX_HOLDERS_TO_FETCH=200
HOLDER_CONCENTRATION_THRESHOLD_HIGH=0.5
HOLDER_CONCENTRATION_THRESHOLD_MEDIUM=0.3
MIN_HOLDERS_COUNT_THRESHOLD=10
MIN_LIQUIDITY_THRESHOLD_ERROR=1000.0
MIN_LIQUIDITY_THRESHOLD_WARNING=5000.0

# Risk Management
# ==============
RUGPULL_PRICE_TIMEFRAME=15m
RUGPULL_PRICE_LIMIT=96
RUGPULL_TRANSACTION_LIMIT=200
RUGPULL_LIQUIDITY_TIMEFRAME=15m
RUGPULL_LIQUIDITY_LIMIT=96
RUGPULL_PRICE_DROP_THRESHOLD=-0.50
RUGPULL_LIQUIDITY_DROP_THRESHOLD=-0.30

# Price Impact Thresholds
# =======================
PRICE_IMPACT_USD_AMOUNT_1K=1000.0
PRICE_IMPACT_THRESHOLD_HIGH_1K=0.05
PRICE_IMPACT_USD_AMOUNT_10K=10000.0
PRICE_IMPACT_THRESHOLD_HIGH_10K=0.10

# Advanced Trading
# ===============
SIGNAL_EXPIRY_SECONDS=300
PRICE_CHECK_INTERVAL_SECONDS=30
ORDER_UPDATE_INTERVAL_SECONDS=15
EXECUTE_MARKET_ORDERS=True
AUTO_CLOSE_POSITIONS=True
TRANSACTION_CONFIRMATION_TIMEOUT_SECONDS=120
TRANSACTION_CONFIRMATION_POLL_INTERVAL_SECONDS=5

# Market Data Cache
# ================
MARKET_DATA_CACHE_TTL_SECONDS=60
MARKET_DATA_CACHE_MAX_SIZE=1000

# Prediction Engine
# ================
PREDICTION_MODEL_DIR=models
PREDICTION_DATA_DIR=data/prediction_data
ENCRYPTION_KEY=change-this-32-char-encryption-key-fernet
ENCRYPTED_SOLANA_PK=dummy_encrypted_private_key_placeholder
ENCRYPTED_SOLANA_PK=dummy_encrypted_pk_placeholder
